  {
    "name": "Github Giphy",
    "version": "1.0",
    "description": "Easily add Gifs to your github comments!",
    "content_scripts": [
      {
        "matches": [
          "https://github.com/*"
        ],
        "css": [
          "dist/style.css"
        ],
        "js": [
          "dist/index.js"
        ]
      }
    ],
    "web_accessible_resources": [
      "src/images/*.png"
    ],
    "manifest_version": 2
  }
